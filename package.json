{
  "name": "makeitbot",
  "version": "0.2.0",
  "private": true,
  "author": "Pat Sissons <patricksissons@gmail.com>",
  "description": "Makes things happen",
  "dependencies": {
    "hubot": "^2.18.0",
    "hubot-darksky": "^1.0.2",
    "hubot-diagnostics": "0.0.1",
    "hubot-giphy-gifme": "^0.1.8",
    "hubot-google-images": "^0.2.6",
    "hubot-google-translate": "^0.2.0",
    "hubot-help": "^0.1.3",
    "hubot-heroku-keepalive": "^1.0.2",
    "hubot-maps": "0.0.2",
    "hubot-pugme": "^0.1.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-rules": "^0.1.1",
    "hubot-scripts": "^2.16.2",
    "hubot-shipit": "^0.2.0",
    "hubot-slack": "^3.4.2",
    "hubot-youtube": "^1.0.2"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "connect": "node --require dotenv/config ./node_modules/coffee-script/bin/coffee ./node_modules/hubot/bin/hubot --adapter slack",
    "start": "node --require dotenv/config ./node_modules/coffee-script/bin/coffee ./node_modules/hubot/bin/hubot --adapter shell --name makeit",
    "test": "mocha --compilers coffee:coffee-script --recursive --reporter spec ./test",
    "watch": "mocha --compilers coffee:coffee-script --recursive --reporter spec ./test --watch"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dotenv": "^2.0.0",
    "mocha": "^2.4.5",
